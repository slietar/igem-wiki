.header-brand
  a(href="index.html"): include ../../internal-assets/logo.svg
  a(href="https://epfl.ch" target="_blank"): svg: use(href="assets/icons.svg#epfl")

button(type="button").header-menu: svg: use(href="assets/icons.svg#menu")

nav.header-nav
  ul.header-nav-ul
    li
      a(href="#") Project
      .header-preview(style="--height: 600px;")
        ul
          li: a(href="Description.html").header-item
            .header-item-icon: svg: use(href="assets/icons.svg#description")
            .header-item-title Description
            .header-item-desc Project background, inspiration, etc.

          li: a(href="Protocols.html").header-item
            .header-item-icon: svg: use(href="assets/icons.svg#protocols")
            .header-item-title Protocols
            .header-item-desc PCR, Western Blot, transformation, etc.

          li: a(href="Model.html").header-item
            .header-item-icon: svg: use(href="assets/icons.svg#model")
            .header-item-title Modelling
            .header-item-desc Bioreactor fluid mechanics models

          li: a(href="Design.html").header-item
            .header-item-icon: svg: use(href="assets/icons.svg#design")
            .header-item-title Design
            .header-item-desc Monomer and dimer design

          li: a(href="Safety.html").header-item
            .header-item-icon: svg: use(href="assets/icons.svg#safety")
